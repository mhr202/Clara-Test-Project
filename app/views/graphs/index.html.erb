<div style='text-align: center'>
<br><br><br><br>
  <h1><b>Your Graphs</b></h1>
  <br><br><br><br>
  <div class="row justify-content-center">
    <div class="col-auto">
      <table class="table table-responsive" cellspacing="30" style='background-color: white; border-radius: 10px; padding: 20px;'>
        <thead>
          <tr>
            <th>Id</th>
            <th >Name</th>
            <th colspan="3">Actions</th>
          </tr>
        </thead>
        <tbody>
          <% @graphs.each do |graph| %>
            <tr>
              <td ><%= graph.id %></td>
              <td><%= graph.name %></td>
              <td><%= link_to "Show", graph_path(graph) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<br>